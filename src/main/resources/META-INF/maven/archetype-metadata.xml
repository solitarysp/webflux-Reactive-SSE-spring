<archetype-descriptor
        xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0-SNAPSHOT.0 http://maven.apache.org/xsd/archetype-descriptor-1.0-SNAPSHOT.0.xsd
        http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0-SNAPSHOT.0"
        xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0-SNAPSHOT.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        name="microprofile12">
    <fileSets>
        <fileSet filtered="true" packaged="true">
            <directory>src/main/java</directory>
            <includes>
                <include>**/*.java</include>
            </includes>
        </fileSet>
        <fileSet filtered="true">
            <directory>src/main/resources</directory>
        </fileSet>
        <fileSet>
            <directory></directory>
            <includes>
                <include>.gitignore</include>
                <include>mvnw</include>
                <include>mvnw.cmd</include>
                <include>.mvn/**</include>
            </includes>
        </fileSet>
    </fileSets>

</archetype-descriptor>
